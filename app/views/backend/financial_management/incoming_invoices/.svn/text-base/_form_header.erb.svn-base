
<div id="" class="content_form">
  <% semantic_fields_for @incoming_invoice do |p| %>
    <% p.inputs "Datos de cabecera" do %>
		<%= p.input :account_payable_incoming_invoice_document_type,:input_html => {:onchange => remote_function(:url => set_incoming_invoice_document_backend_financial_management_incoming_invoices_url,:with => "$(this).serialize()")} %>
		<%= p.input :material_purchase_order,:as => :string,:wrapper_html => {:class => "hidden"},:input_html => {:size => 10,:class => "right reset",:name => "account_payable_incoming_invoice[material_purchase_order_id]"}%>
		<hr/>
		<%= p.input    :supplier_id,:as => :string,:wrapper_html => {:class => "hidden reset"},:input_html => {:size => 10} %>
		<%= p.input    :supplier_name,:wrapper_html => {:class => ""},:input_html => {:class => "reset"} %>
                
		<%= p.input    :tenderer_id,:as => :string,:wrapper_html => {:class => "hidden reset"},:input_html => {:size => 10} %>
		<%= p.input    :tenderer_type,:as => :string,:wrapper_html => {:class => "hidden reset"},:input_html => {:size => 10,:value => "Supplier"} %>
		<%= p.input    :tenderer_name,:wrapper_html => {:class => ""},:input_html => {:class => "reset"} %>
		<hr/>
                
		<%= p.input :invoice_date,:as => :string,:input_html => {:readonly => true,:class => "reset"} %>
		<%= p.input :reference,:input_html => {:size => 20,:class => "reset"} %>
		<%= p.input :control_number,:input_html => {:size => 20,:class => "reset"} %>
		<%= p.input :description,:input_html => {:class => "reset"} %>
		
		<%= p.input :currency_type,:wrapper_html => {:class => "hidden"} %>
		<%= p.input :create_by,:wrapper_html => {:class => ""},:input_html => {:disabled => "disabled"} %>
		<%= p.input :posting_date,:wrapper_html => {:class => "hidden"},:as => :string,:input_html => {:disabled => "disabled"}  %>
		<%= p.input :purchase_ledger_period,:wrapper_html => {:class => ""},:as => :string,:input_html => {:type => "type",:size => "5"}  %>
	  <% end %>

		
	 <% p.inputs "Datos complementarios de afectaciÃ³n",:class => "optional" do %>
		<%#= p.input :with_affectation_inventory %>
		<%= p.input :associated_purchase_ledgers %>
		<%= p.input :municipal_tax %>
		<%= p.input :legal_contractual_voluntary_obligation %>
	 <% end %>


  <% end %>
</div>

