  <div class="headerWrap">
    <div class="headerWrapContent">
          <h2>Registrar nueva chequera</h2>
    </div>
  </div>

<div id="" title="Registrar nueva chequera" class="mainWrapContent">
  <% semantic_form_remote_for @checkbook, :update => "#{controller.controller_name}_container",:url => backend_cashbank_checkbooks_url,:html => {:class => "form_generate"} do |f| %>
        <%= render :partial => "form",:locals => {:f => f} %>
  <% end %>
</div>

  <div class="footerWrap">
    <div class="footerWrapContent right">
      <%= button_to_function("Guardar", "$(\"#new_cash_bank_checkbook\").submit()")  %>
    </div>
  </div>
<script type="text/javascript">



    $(document).ready(function(){
        $("button, input:submit,input:button,a.button ").button();
	 $("#cash_bank_checkbook_initial_check_number,#cash_bank_checkbook_final_check_number").masked("999999");
	  $( "#new-element-dialog-form" ).dialog({
		  autoOpen: true,
		  height: 300,
		  width: 320,
		  modal: true,
		  buttons: {
			 "Asociar nueva chequera": function() {
				var bValid = true;
				if ( bValid ) {
				  $("#new-element-dialog-form form").submit();


				}
			 },
			 "Cancelar": function() {
				$( this ).dialog( "destroy" );
			 }
		  },
		  close: function() {
			 //allFields.val( "" ).removeClass( "ui-state-error" );
		  }
	 });
});
</script>