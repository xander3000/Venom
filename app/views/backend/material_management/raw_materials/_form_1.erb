<div id="" class="content_form">
  <% f.inputs "",:class => "no-border" do %>
		<div>
		  <div class="c15l subc">
                          <%= f.input :code,:label => false,:input_html => {:size => 10,:disabled => true,:class => "top"} %>

		  </div>
		  <div class="c70r subc">
                          <%=  f.input :name,:label => false,:input_html => {:placeholder => "Nombre del material",:class => "top not-editable"} %>
		  </div>
		</div>
  <% end %>
  <% f.inputs "Datos generales" do %>
		<div>
		  <div class="c50l subc">
                    <% f.inputs "" do %>
			 <%= f.input :material_base_measure_unit,:input_html => {:class =>"not-editabble" } %>
                         <%= f.input :old_id,:as => :string,:input_html => {} %>
                    <%  end %>
				
			 
		  </div>
		  <div class="c50r subc">
                    <% f.inputs "" do %>
								<%= f.input :material_raw_material_category,:input_html => {:class =>"not-editabble"} %>
								<%= f.input :material_external_raw_material_category,:input_html => {:class =>"not-editabble"} %>
								<%= f.input :valid_from,:input_html => {:size => 10} %>
                    <% end %>
		  </div>
		</div>
  <% end %>
  <% f.inputs "Dimensiones/EAN" do %>
		<div>
		  <div class="c50l subc">
                    <% f.inputs "" do %>
			  <%= f.input :gross_weight,:input_html => {:class => "right",:size => 10} %>
                          <%= f.input :net_weight,:input_html => {:class => "right",:size => 10} %>
                          <%= f.input :volume,:input_html => {:class => "right",:size => 10} %>
                          <%= f.input :side_dimension,:input_html => {} %>
                          <%= f.input :ean_upc_code,:input_html => {:class => "right",:size => 10} %>
                    <%  end %>


		  </div>
		  <div class="c50r subc">
                    <% f.inputs "" do %>
			<%= f.input :material_weight_unit,:input_html => {} %>
                        <li><label>&nbsp;</label></li>
                        <%= f.input :material_volume_unit,:input_html => {} %>
                        <li><label>&nbsp;</label></li>
                        <%= f.input :material_ean_category,:input_html => {} %>
                    <% end %>
		  </div>
		</div>
  <% end %>
</div>