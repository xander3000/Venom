@import"../subcolumns.css";
@import"../general-styles.css";

@font-face {
  font-family: 'FontOpensSans';
  src: url(../fonts/OpenSans-Regular.ttf);
  font-weight: normal;
  font-style: normal;
}

@font-face {
  font-family: 'FontOpensSansLight';
  src: url(../fonts/OpenSans-Light.ttf);
  font-weight: normal;
  font-style: normal;
}


*
{
    margin:0;
    padding:0;
}

html * {
    font-size: 100.01%;
}

body
{
    /*font: 13px arial,sans-serif;*/
    font-size: 11px;
	 font-family: sans-serif,Arial;
    background: #000;
}

a
{
    color: #2424FF;
}

p
{
    color: #282828;
    font-size: 1.1em;
    letter-spacing: 0.03em;
    line-height: 1.6em;
    margin: 0 0 1em;
}

strong, b {
    font-weight: bold;
}

h1, h2, h3, h4, h5, h6 {
    color: #5C708A;
    font-weight: bold;
    margin: 0 0 0.25em;
}

h2
{
    font-size: 200%;
    font-weight: lighter;
    margin: 0 0 5px;
}

h3
{
    color: #000000;
    font-size: 150%;
}

hr
{
	color: #fff;
	background:transparent;
	margin: 0 0 0.5em 0;
	padding: 0 0 0.5em 0;
	border:0;
	border-bottom: 1px #eee solid;;
}

img{
    border: none;
}

.highlight{
	font-size: 1.5em;
	font-weight: bold;
}

/*
    BEGIN: Table
*/
table
{
    border-collapse: collapse;
    /*margin: 0.5em 0 0.2em 0;*/
    width: 100%;
    border: 1px solid #D3D3D3;
}

table.no-marging{
    margin:0;
}

th, td {
    padding: 0.5em;
}

th.no-padding, td.no-padding {
    padding: 0;
}

table.display tr.odd td, table.display tr.even td{
 padding: 3px 10px;
	cursor: pointer;
}

table.display tr th,table.display tr td{
	 border-left: 1px solid #C2C2C2;
	 border-right: 1px solid #C2C2C2;
}

table.no-border tr th,table.no-border tr td{
	 border: none;
}


table th, table td.header {
    background: none repeat scroll 0 0 #EBEBEB;
    color: #666666;
    text-align: center;
	 font-weight: bolder;
}

table tr.item:hover
{
    background: #FAEE8D;
}

table tr.odd
{
    background: #FFF;
}

table tr.ever
{
    background: #EFF2F4;
}

table tr.odd td, table tr.ever td
{
	padding-top: 1.5em;
	padding-bottom:  1.5em;

}

table tr.border-top
{
    border-top: 1px solid #D2D2D2;
}
table.no-border,
table tr.no-border,
table td.no-border
{
    border-color: #FFF
}
table.no-border-top,
table tr.no-border-top,
table td.no-border-top
{
    border-top-color: #FFF
}
table.no-border-bottom,
table tr.no-border-bottom,
table td.no-border-bottom
{
    border-bottom-color: #FFF
}

table tr.no-border-right,
table td.no-border-right
{
    border-right-color: #FFF
}

#table-container
{
    width: 350px;
}

#table-container #table-body div.item
{
    border: 1px solid #CFCFCF;
    margin-bottom: 3px;
    width: 100%;
}
#table-container #table-body div.item table
{
 margin-top: 5px;
 border: none;
}
#table-container #table-body div.item td
{
    border-left: 1px solid #CFCFCF;
    font-size: 1em;
}

#table-container #table-body div.item td.first
{
    border-left: none;
    color: #999999;
    font-size: 1.05em;
    font-weight: bold;
}

/*
    END: Table
*/


/*
    BEGIN: .top-action
*/
.top-actions {
    padding-bottom: 5px;
}

.top-actions a {
    text-decoration: none;
}

.top-actions input[type="button"], .top-actions input.button[type="button"], .top-actions input[type="submit"], .top-actions input.button[type="submit"] {
    background-color: transparent;
    border: 0 none;
    color: #2424FF;
    font-size: 1em;
    font-weight: normal;
    margin: 0;
    padding: 0;
    background: none;
    cursor: pointer;
}
/*
    END: .top-action
*/

/*
    BEGIN: borders
*/

    .border-left
    {
        border-left: 1px solid #D2D2D2;
    }
    .border-top
    {
        border-top: 1px solid #D2D2D2;
    }

    .border-bottom
    {
        border-bottom:  1px solid #D2D2D2;
    }

    .border-right
    {
        border-right:  1px solid #D2D2D2;
    }



/*
    END: borders
*/


input.hasDatepicker{
	width: 100px;
}

.confirm-message, .help-message {
    background-color: #FFFFD5;
    border: 1px solid #FFD700;
    margin-bottom: 5px;
    padding: 10px;
}

.unexpired_order{
    color: black;
    background: green;
}

.expired_order{
    color: black;
    background: red;
}

.warning_order{
    color: black;
    background: yellow;
}

.color_unexpired_order,.green{
    color: green;
}
.color_expired_order,.red{
    color: red;
}
.color_warning_order{
    color: yellow;
}


.box_status_order{
    margin: 0 5px;
    padding: 5px 10px;
    cursor: help
}

.info{
 background: transparent url(/images/icons/information.png) center right no-repeat;
 padding-right: 25px;
 cursor: pointer;
}

.tooltip {
	display:none;
	background:transparent url(/images/global/black_arrow_big.png);
	    -moz-box-shadow:1px 1px 4px #666;
    -webkit-box-shadow:1px 1px 4px #666;
    box-shadow:1px 1px 4px #666;
	 background: white;
	 border: 1px solid #E3E3E3;
	font-size:12px;
	
	height:auto;;
	width:auto;
	padding:5px;
	color:#000;
	z-index: 10000;
}

#print_to_menu{
    width: 55px;
    display: none;
    position: absolute;
    right: 25px;
     -moz-border-radius:0 0 4px 4px;
    -webkit-border-radius:0 0 4px 4px;
    border-radius:0 0 4px 4px ;
    border-color: #D3D3D3;
   
}

#loading
{
    position: fixed;
    z-index: 10000;
    width: 128px;
    height: 128px;
    padding-top: 5px;
    top: 205px;
    left: 45%;
    display: none;
