html{
	font-family: "courier new";
	font-size: 0.8em;
	margin: 0;
	padding: 0;
}
#header{
	margin-bottom: 5px;
}
p{
	padding: 0;
	margin: 0;
	line-height: 15px;

}
table {
	border-collapse: collapse;
	 border: 1px solid #D3D3D3;
    border-collapse: collapse;
    margin: 0.5em 0 0.2em;
    width: 100%;
}

table td, table th {
    border-color: #D2D2D2;
}
table th {
    background: none repeat scroll 0 0 #EBEBEB;
    color: #666666;
    text-align: center;
}
th, td {
    padding: 0.5em;
}

table caption{
	display: none;
}

