/*button icon element(s) */

input.ui-button,.ui-button-text-only .ui-button-text{
    padding: 0.4em;
    text-decoration: underline;
}



.ui-state-default, .ui-widget-content .ui-state-default, .ui-widget-header .ui-state-default{
    background: none repeat scroll 0 0  #D0D2D3;
    color: #363436;
    text-decoration: underline;
    
    /*text-transform: uppercase;*/
}
.ui-state-default.secondary, .ui-widget-content .ui-state-default.secondary, .ui-widget-header .ui-state-default.secondary{
    background: none repeat scroll 0 0 #363436;
    color: #FFFFFF;
}
.ui-state-default.no-background, .ui-widget-content .ui-state-default.no-background, .ui-widget-header .ui-state-default.no-background{
    background: none;;
}

/* no icon support for input elements, provide padding by default */

.ui-corner-all, .ui-corner-bottom, .ui-corner-right, .ui-corner-br {
    border-bottom-right-radius: 0px;
    border-bottom: none;
}

.ui-corner-all, .ui-corner-bottom, .ui-corner-left, .ui-corner-bl {
    border-bottom-left-radius: 0px;
    border-left: none;
}

.ui-corner-all, .ui-corner-top, .ui-corner-right, .ui-corner-tr {
    border-top-right-radius: 0px;
    border-right:  none;
}

.ui-corner-all, .ui-corner-top, .ui-corner-left, .ui-corner-tl {
    border-top-left-radius: 0px;
    border-top: none;
}

.ui-button{
     text-decoration: underline;
}

