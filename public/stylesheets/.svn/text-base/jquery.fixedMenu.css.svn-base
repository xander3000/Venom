
.menunav{
      font: 13px/27px Arial,sans-serif;
      color:#CCCCCC !important;
      height:30px;
}
.menunav a:hover{
    background-color:#F1F1F1;
	 color: #000;
}
.menunav a {
    text-decoration: none;
    padding: 4px 8px 7px;
    color:#CCCCCC;
    outline:none;
	 
}
.menunav ul{
    list-style: none;
    margin:0;
    padding:0 0 0 10px;
}
.menunav ul li{
    padding:0;
    float:left;
    
}
.menunav ul li ul li{
    padding:0;
    float:none;
    margin: 0 0 0 0px;
    width:100%;
}
.menunav ul li ul{
    position: absolute;
    border: 1px solid #ccc;
    box-shadow: 0 1px 5px #CCCCCC;
    margin-top: -1px;
    display:none;
    padding: 0px 16px 0px 0;
}
.active ul{
    display:block !important; 
}
.active a{
    background-color: white;
    border: 1px solid #ccc;
    border-bottom: 0;
    box-shadow: 0 -1px 5px #e4ebf8;
    display: block;
    height: 29px;
    padding: 0 8px 0 8px;
    position:relative;
    z-index: 1;
}
.active a:hover{
    background-color:#ccc;
}
.active ul a:hover{
    background-color:white;
	 color: #000;
}

.active ul a{
    border: 0 !important;
    box-shadow: 0 0 0 #CCCCCC;
    border:0;
    width: 100%;
}
.arrow {
    border-color: #CCCCCC transparent transparent;
    border-style: solid dashed dashed;
    margin-left: 5px;
    position: relative;
    top: 10px;
}


